
<div class="card card-main-form">
<div class="card-body form-card">
<%= form_tag(access_attempt_login_path, :method => :post) do %>

<div class="form-group">
    
        <%= label_tag(:username)%>
        <%= text_field_tag(:username, params[:username])%>
    
</div>
<div class="form-group">
    
    <%= label_tag(:password)%>
    <%= password_field_tag(:password)%>
    
</div>
    &nbsp;
    <%= submit_tag("Log In")%>
</table>
<% end %>
</div>
</div>