<h1><%=@staff.name%></h1>
<h2><%=@staff.title%></h2>

<h3>Questions</h3>
<%@staff.posts.each do |post|%>
    <u><%= link_to post.title, post_path(post), class: 'link' %></u><br>
    <%=post.content%><br>
    ---Answers---<br>
    <%post.solutions.each do |solution|%>
        <%= solution.content%><br>
    <%end%><br><br>
<%end%>

